<%- include('../layouts/layout', {
    title: 'Dashboard',
    bodyClass: 'd-flex flex-column min-vh-100',
    headerType: 'dashboard',
    body: `
        <main class="flex-grow-1 p-4 bg-light">
            <div class="container">
                <!-- Dashboard Header -->
                <div class="d-flex justify-content-between align-items-center mb-4 flex-wrap gap-3">
                    <div>
                        <h3 class="mb-1">Dashboard</h3>
                        <p class="text-muted mb-0" id="userRoleDisplay">Loading...</p>
                    </div>
                    <div class="d-flex gap-2" id="adminActions" style="display: none !important;">
                        <button class="btn btn-outline-primary" onclick="window.location.href='/admin/users'">
                            <i class="bi bi-people me-2"></i>Manage Users
                        </button>
                        <button class="btn btn-primary" onclick="window.location.href='/survey'">
                            <i class="bi bi-plus-lg me-2"></i>New Entry
                        </button>
                    </div>
                </div>

                <!-- Statistics Cards -->
                <div class="row g-4 mb-4">
                    <div class="col-md-3 col-sm-6">
                        <div class="card border-0 shadow-sm h-100">
                            <div class="card-body d-flex align-items-center">
                                <div class="rounded-circle bg-primary bg-opacity-10 p-3 me-3">
                                    <i class="bi bi-people-fill text-primary fs-4"></i>
                                </div>
                                <div>
                                    <h6 class="text-muted mb-1">Total Participants</h6>
                                    <h4 class="mb-0" id="totalParticipants">-</h4>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6">
                        <div class="card border-0 shadow-sm h-100">
                            <div class="card-body d-flex align-items-center">
                                <div class="rounded-circle bg-success bg-opacity-10 p-3 me-3">
                                    <i class="bi bi-house-door-fill text-success fs-4"></i>
                                </div>
                                <div>
                                    <h6 class="text-muted mb-1">Households</h6>
                                    <h4 class="mb-0" id="totalHouseholds">-</h4>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6">
                        <div class="card border-0 shadow-sm h-100">
                            <div class="card-body d-flex align-items-center">
                                <div class="rounded-circle bg-info bg-opacity-10 p-3 me-3">
                                    <i class="bi bi-geo-alt-fill text-info fs-4"></i>
                                </div>
                                <div>
                                    <h6 class="text-muted mb-1">Parishes</h6>
                                    <h4 class="mb-0" id="totalParishes">-</h4>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6">
                        <div class="card border-0 shadow-sm h-100">
                            <div class="card-body d-flex align-items-center">
                                <div class="rounded-circle bg-warning bg-opacity-10 p-3 me-3">
                                    <i class="bi bi-calendar-check-fill text-warning fs-4"></i>
                                </div>
                                <div>
                                    <h6 class="text-muted mb-1">This Month</h6>
                                    <h4 class="mb-0" id="thisMonthCount">-</h4>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Search and Table Section -->
                <div class="card shadow-sm">
                    <div class="card-header bg-white py-3">
                        <div class="d-flex justify-content-between align-items-center flex-wrap gap-3">
                            <div class="d-flex align-items-center gap-3 flex-grow-1" style="max-width: 600px;">
                                <div class="position-relative flex-grow-1">
                                    <input class="form-control" type="text" id="searchInput" placeholder="Search by name..." autocomplete="off">
                                    <div id="autocompleteList" class="position-absolute w-100 shadow rounded bg-white d-none" style="z-index: 1050; max-height: 300px; overflow-y: auto"></div>
                                </div>
                                <button class="btn btn-outline-secondary" type="button" data-bs-toggle="collapse" data-bs-target="#filterSection">
                                    <i class="bi bi-funnel me-1"></i>Filters
                                </button>
                            </div>
                        </div>
                        
                        <!-- Filter Section -->
                        <div class="collapse mt-3" id="filterSection">
                            <div class="row g-2">
                                <div class="col-md-3 col-sm-6">
                                    <select class="form-select form-select-sm" id="filterOccupation">
                                        <option value="">All Occupations</option>
                                        <option value="Farming">Farming</option>
                                        <option value="Tenant">Tenant</option>
                                        <option value="Fishing">Fishing</option>
                                        <option value="Vending">Vending</option>
                                        <option value="Hired Labor">Hired Labor</option>
                                        <option value="Employed">Employed</option>
                                        <option value="OFW">OFW</option>
                                        <option value="Domestic Worker">Domestic Worker</option>
                                        <option value="Entertainment">Entertainment</option>
                                    </select>
                                </div>
                                <div class="col-md-3 col-sm-6">
                                    <input type="text" class="form-control form-control-sm" id="filterAddress" placeholder="Filter by Address...">
                                </div>
                                <div class="col-md-3 col-sm-6">
                                    <select class="form-select form-select-sm" id="filterParish">
                                        <option value="">All Parishes</option>
                                    </select>
                                </div>
                                <div class="col-md-3 col-sm-6">
                                    <select class="form-select form-select-sm" id="filterRelation">
                                        <option value="">All Relations</option>
                                        <option value="HH Head">HH Head</option>
                                        <option value="Spouse">Spouse</option>
                                        <option value="Member">Member</option>
                                    </select>
                                </div>
                                <div class="col-md-3 col-sm-6">
                                    <div class="input-group input-group-sm">
                                        <span class="input-group-text">Age</span>
                                        <input type="number" class="form-control" id="filterMinAge" placeholder="Min">
                                        <input type="number" class="form-control" id="filterMaxAge" placeholder="Max">
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-6">
                                    <button type="button" class="btn btn-sm btn-outline-secondary w-100" id="clearFilters">
                                        <i class="bi bi-x-circle me-1"></i>Clear Filters
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="table-responsive">
                        <table class="table table-hover align-middle mb-0" id="participantsTable">
                            <thead class="table-light">
                                <tr>
                                    <th>Name</th>
                                    <th>Relation</th>
                                    <th>Address</th>
                                    <th>Parish</th>
                                    <th>Age</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody id="participantsTableBody">
                                <tr><td colspan="6" class="text-center py-4">Loading participants...</td></tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="card-footer bg-white d-flex justify-content-between align-items-center">
                        <small class="text-muted" id="showingInfo">Showing 0 entries</small>
                        <nav aria-label="Table pagination">
                            <ul class="pagination pagination-sm mb-0" id="pagination"></ul>
                        </nav>
                    </div>
                </div>
            </div>
            
            <!-- Participant Details Modal -->
            <div class="modal fade" id="participantModal" tabindex="-1" aria-hidden="true">
                <div class="modal-dialog modal-xl">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title">Participant Details</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div id="accessIndicator"></div>
                            
                            <!-- General Information Section -->
                            <div class="card mb-3">
                                <div class="card-header bg-light py-2">
                                    <h6 class="mb-0"><i class="bi bi-person me-2"></i>General Information</h6>
                                </div>
                                <div class="card-body py-2">
                                    <div class="row g-2">
                                        <div class="col-md-3"><strong>Purok/Gimong:</strong> <span id="gen-purok">-</span></div>
                                        <div class="col-md-3"><strong>Barangay:</strong> <span id="gen-barangay">-</span></div>
                                        <div class="col-md-3"><strong>Municipality:</strong> <span id="gen-municipality">-</span></div>
                                        <div class="col-md-3"><strong>Province:</strong> <span id="gen-province">-</span></div>
                                        <div class="col-md-3"><strong>Parish:</strong> <span id="gen-parish">-</span></div>
                                        <div class="col-md-3"><strong>Diocese:</strong> <span id="gen-diocese">-</span></div>
                                        <div class="col-md-3"><strong>Years of Residency:</strong> <span id="gen-residency">-</span></div>
                                        <div class="col-md-3"><strong>Family Members:</strong> <span id="gen-familyMembers">-</span></div>
                                        <div class="col-md-3"><strong>Family Structure:</strong> <span id="gen-familyStructure">-</span></div>
                                        <div class="col-md-3"><strong>Local Dialect:</strong> <span id="gen-dialect">-</span></div>
                                        <div class="col-md-3"><strong>Ethnicity:</strong> <span id="gen-ethnicity">-</span></div>
                                        <div class="col-md-3"><strong>Transportation:</strong> <span id="gen-transportation">-</span></div>
                                        <div class="col-md-3"><strong>Road Structure:</strong> <span id="gen-road">-</span></div>
                                        <div class="col-md-3"><strong>Classification:</strong> <span id="gen-classification">-</span></div>
                                    </div>
                                </div>
                            </div>

                            <!-- Family Members Section -->
                            <div class="card mb-3">
                                <div class="card-header bg-light py-2">
                                    <h6 class="mb-0"><i class="bi bi-people me-2"></i>Family Members</h6>
                                </div>
                                <div class="card-body py-2">
                                    <div class="table-responsive">
                                        <table class="table table-sm table-bordered mb-0">
                                            <thead>
                                                <tr>
                                                    <th>Name</th>
                                                    <th>Relation</th>
                                                    <th>Sex</th>
                                                    <th>Age</th>
                                                    <th>Civil Status</th>
                                                    <th>Religion</th>
                                                    <th>Sacraments</th>
                                                    <th>Studying</th>
                                                    <th>Education</th>
                                                    <th>Occupation</th>
                                                    <th>Work Status</th>
                                                    <th>Organization</th>
                                                    <th>Position</th>
                                                </tr>
                                            </thead>
                                            <tbody id="modalFamilyTable"></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                            <!-- Health & Living Conditions Section -->
                            <div class="card mb-3">
                                <div class="card-header bg-light py-2">
                                    <h6 class="mb-0"><i class="bi bi-heart-pulse me-2"></i>Health & Living Conditions</h6>
                                </div>
                                <div class="card-body py-2">
                                    <div class="row g-2">
                                        <div class="col-md-4"><strong>Common Illnesses:</strong> <span id="health-illness">-</span></div>
                                        <div class="col-md-4"><strong>Treatment Source:</strong> <span id="health-treatment">-</span></div>
                                        <div class="col-md-4"><strong>Water Source:</strong> <span id="health-water">-</span></div>
                                        <div class="col-md-4"><strong>Lighting Source:</strong> <span id="health-lighting">-</span></div>
                                        <div class="col-md-4"><strong>Cooking Source:</strong> <span id="health-cooking">-</span></div>
                                        <div class="col-md-4"><strong>Garbage Disposal:</strong> <span id="health-garbage">-</span></div>
                                        <div class="col-md-4"><strong>Toilet Facility:</strong> <span id="health-toilet">-</span></div>
                                        <div class="col-md-4"><strong>Toilet Distance:</strong> <span id="health-toiletDist">-</span></div>
                                    </div>
                                </div>
                            </div>

                            <!-- Socio-Economic Section -->
                            <div class="card mb-3">
                                <div class="card-header bg-light py-2">
                                    <h6 class="mb-0"><i class="bi bi-currency-exchange me-2"></i>Socio-Economic</h6>
                                </div>
                                <div class="card-body py-2">
                                    <div class="row g-2">
                                        <div class="col-md-3"><strong>Monthly Income:</strong> <span id="soc-income">-</span></div>
                                        <div class="col-md-3"><strong>Weekly Expenses:</strong> <span id="soc-expenses">-</span></div>
                                        <div class="col-md-3"><strong>Has Savings:</strong> <span id="soc-savings">-</span></div>
                                        <div class="col-md-3"><strong>Savings Location:</strong> <span id="soc-savingsLoc">-</span></div>
                                        <div class="col-md-3"><strong>House Ownership:</strong> <span id="soc-ownership">-</span></div>
                                        <div class="col-md-3"><strong>House Classification:</strong> <span id="soc-houseClass">-</span></div>
                                        <div class="col-md-3"><strong>Land Area (ha):</strong> <span id="soc-landArea">-</span></div>
                                        <div class="col-md-3"><strong>Distance of House from Church:</strong> <span id="soc-church">-</span></div>
                                        <div class="col-md-3"><strong>Distance of House from Market:</strong> <span id="soc-market">-</span></div>
                                        <div class="col-md-3"><strong>Organizations:</strong> <span id="soc-organizations">-</span></div>
                                        <div class="col-md-3"><strong>Livestock:</strong> <span id="soc-livestock">-</span></div>
                                        <div class="col-md-3"><strong>Assets:</strong> <span id="soc-assets">-</span></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-primary d-none" id="editBtn">
                                <i class="bi bi-pencil me-2"></i>Edit
                            </button>
                            <button type="button" class="btn btn-danger d-none" id="deleteBtn">
                                <i class="bi bi-trash me-2"></i>Delete
                            </button>
                        </div>
                    </div>
                </div>
            </div>
            
            <!-- Edit Participant Modal -->
            <div class="modal fade" id="editModal" tabindex="-1" aria-hidden="true">
                <div class="modal-dialog modal-xl">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title">Edit Participant</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <form id="editForm">
                                <!-- General Information -->
                                <div class="card mb-3">
                                    <div class="card-header bg-light py-2">
                                        <h6 class="mb-0"><i class="bi bi-person me-2"></i>General Information</h6>
                                    </div>
                                    <div class="card-body py-2">
                                        <div class="row g-2">
                                            <div class="col-md-3">
                                                <label class="form-label">Purok/Gimong</label>
                                                <input type="text" class="form-control form-control-sm" name="purok_gimong" id="edit-purok">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="form-label">Barangay</label>
                                                <input type="text" class="form-control form-control-sm" name="barangay_name" id="edit-barangay">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="form-label">Municipality</label>
                                                <input type="text" class="form-control form-control-sm" name="municipality" id="edit-municipality">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="form-label">Province</label>
                                                <input type="text" class="form-control form-control-sm" name="province" id="edit-province">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="form-label">Parish</label>
                                                <input type="text" class="form-control form-control-sm" name="parish_name" id="edit-parish">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="form-label">Years of Residency</label>
                                                <input type="number" class="form-control form-control-sm" name="years_residency" id="edit-residency">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="form-label">Family Members</label>
                                                <input type="number" class="form-control form-control-sm" name="num_family_members" id="edit-familyMembers">
                                            </div>
                                            <div class="col-md-3">
                                                <label class="form-label">Family Structure</label>
                                                <input type="text" class="form-control form-control-sm" name="family_structure" id="edit-familyStructure">
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- Family Members -->
                                <div class="card mb-3">
                                    <div class="card-header bg-light py-2">
                                        <h6 class="mb-0"><i class="bi bi-people me-2"></i>Family Members</h6>
                                    </div>
                                    <div class="card-body py-2">
                                        <div class="table-responsive">
                                            <table class="table table-sm table-bordered mb-0" id="editFamilyTable">
                                                <thead>
                                                    <tr>
                                                        <th>Name</th>
                                                        <th>Relation</th>
                                                        <th>Sex</th>
                                                        <th>Age</th>
                                                        <th>Civil Status</th>
                                                        <th>Religion</th>
                                                        <th>Sacraments</th>
                                                        <th>Studying</th>
                                                        <th>Education</th>
                                                        <th>Occupation</th>
                                                        <th>Work Status</th>
                                                        <th>Organization</th>
                                                        <th>Position</th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>

                                <!-- Health & Living Conditions -->
                                <div class="card mb-3">
                                    <div class="card-header bg-light py-2">
                                        <h6 class="mb-0"><i class="bi bi-heart-pulse me-2"></i>Health & Living Conditions</h6>
                                    </div>
                                    <div class="card-body py-2">
                                        <div class="row g-2">
                                            <div class="col-md-4">
                                                <label class="form-label">Water Source</label>
                                                <select class="form-select form-select-sm" name="potable_water_source_code" id="edit-water">
                                                    <option value="">Select...</option>
                                                    <option value="1">Local Water System</option>
                                                    <option value="2">Spring Water</option>
                                                    <option value="3">River</option>
                                                    <option value="4">Open Well</option>
                                                    <option value="5">Own Artesian Well</option>
                                                    <option value="6">Community Artesian Well</option>
                                                    <option value="7">Community Faucet</option>
                                                    <option value="8">Electric Deep Well</option>
                                                    <option value="9">Free Flowing Water Pipe</option>
                                                    <option value="10">Rain Water</option>
                                                    <option value="11">Water Refilling Station</option>
                                                    <option value="12">NAWASA</option>
                                                    <option value="99">Others</option>
                                                </select>
                                            </div>
                                            <div class="col-md-4">
                                                <label class="form-label">Lighting Source</label>
                                                <select class="form-select form-select-sm" name="lighting_source_code" id="edit-lighting">
                                                    <option value="">Select...</option>
                                                    <option value="1">Electricity</option>
                                                    <option value="2">Kerosene (Gaas)</option>
                                                    <option value="3">LPG</option>
                                                    <option value="4">Solar Panel / Solar Lamp</option>
                                                    <option value="5">Battery</option>
                                                    <option value="6">Generator</option>
                                                    <option value="7">None</option>
                                                    <option value="99">Others</option>
                                                </select>
                                            </div>
                                            <div class="col-md-4">
                                                <label class="form-label">Cooking Source</label>
                                                <select class="form-select form-select-sm" name="cooking_source_code" id="edit-cooking">
                                                    <option value="">Select...</option>
                                                    <option value="1">Woods</option>
                                                    <option value="2">Charcoal</option>
                                                    <option value="3">Kerosene (Gas)</option>
                                                    <option value="4">LPG</option>
                                                    <option value="5">Electricity</option>
                                                    <option value="99">Others</option>
                                                </select>
                                            </div>
                                            <div class="col-md-4">
                                                <label class="form-label">Toilet Facility</label>
                                                <select class="form-select form-select-sm" name="toilet_facility_code" id="edit-toilet">
                                                    <option value="">Select...</option>
                                                    <option value="1">Water-Sealed Flush (Own)</option>
                                                    <option value="2">Water-Sealed Flush (Shared)</option>
                                                    <option value="3">Closed Pit</option>
                                                    <option value="4">Open Pit</option>
                                                    <option value="5">No Toilet</option>
                                                    <option value="99">Others</option>
                                                </select>
                                            </div>
                                            <div class="col-md-4">
                                                <label class="form-label">Garbage Disposal</label>
                                                <select class="form-select form-select-sm" name="garbage_disposal_code" id="edit-garbage">
                                                    <option value="">Select...</option>
                                                    <option value="1">Segregating Waste</option>
                                                    <option value="2">Collected by Garbage Truck</option>
                                                    <option value="3">Recycling / Reusing at Home</option>
                                                    <option value="4">Selling / Giving Away Recyclables</option>
                                                    <option value="5">Composting</option>
                                                    <option value="6">Burning</option>
                                                    <option value="7">Dumping in Pit with Cover</option>
                                                    <option value="8">Dumping in Pit without Cover</option>
                                                    <option value="9">Throwing in Uninhabited Locations</option>
                                                    <option value="99">Others</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- Socio-Economic -->
                                <div class="card mb-3">
                                    <div class="card-header bg-light py-2">
                                        <h6 class="mb-0"><i class="bi bi-currency-exchange me-2"></i>Socio-Economic</h6>
                                    </div>
                                    <div class="card-body py-2">
                                        <div class="row g-2">
                                            <div class="col-md-3">
                                                <label class="form-label">Monthly Income</label>
                                                <select class="form-select form-select-sm" name="income_monthly_code" id="edit-income">
                                                    <option value="">Select...</option>
                                                    <option value="1">₱3,000 and below</option>
                                                    <option value="2">₱3,001 - ₱6,000</option>
                                                    <option value="3">₱6,001 - ₱9,000</option>
                                                    <option value="4">₱9,001 - ₱12,000</option>
                                                    <option value="5">₱12,001 - ₱15,000</option>
                                                    <option value="6">₱15,001 - ₱18,000</option>
                                                    <option value="7">₱18,001 - ₱21,000</option>
                                                    <option value="8">₱21,001 - ₱24,000</option>
                                                    <option value="9">₱24,001 - ₱27,000</option>
                                                    <option value="10">₱27,001 - ₱30,000</option>
                                                    <option value="11">₱30,001 and up</option>
                                                </select>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="form-label">Weekly Expenses</label>
                                                <select class="form-select form-select-sm" name="expenses_weekly_code" id="edit-expenses">
                                                    <option value="">Select...</option>
                                                    <option value="1">₱300 and below</option>
                                                    <option value="2">₱301 - ₱600</option>
                                                    <option value="3">₱601 - ₱900</option>
                                                    <option value="4">₱901 - ₱1,200</option>
                                                    <option value="5">₱1,201 - ₱1,500</option>
                                                    <option value="6">₱1,501 - ₱1,800</option>
